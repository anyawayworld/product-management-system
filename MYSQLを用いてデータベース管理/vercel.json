{
  "version": 2,
  "builds": [
    {
      "src": "app/MYSQLを用いてデータベース管理/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.herokuapp.com/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/app/MYSQLを用いてデータベース管理/frontend/dist/$1"
    }
  ],
  "functions": {
    "app/MYSQLを用いてデータベース管理/frontend/dist/index.html": {
      "includeFiles": "app/MYSQLを用いてデータベース管理/frontend/dist/**"
    }
  }
}
